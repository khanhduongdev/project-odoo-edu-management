<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_edu_session" model="ir.sequence">
            <field name="name">Session Code</field>
            <field name="code">edu.session</field>
            <field name="prefix">SESS/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_edu_subject" model="ir.sequence">
            <field name="name">Subject Code</field>
            <field name="code">edu.subject</field>
            <field name="prefix">SJ</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
